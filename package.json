  "angular-cli": {},
  "scripts": {
    "rimraf": "rimraf",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "typings": "typings",
    "webpack": "webpack",
    "clean": "npm cache clean && npm run rimraf -- node_modules coverage dist",
    "clean.dist": "npm run rimraf -- dist/*.*",
    "preclean.install": "npm run clean",
    "clean.install": "npm set progress=false && npm install",
    "preclean.start": "npm run clean",
    "clean.start": "npm start",
    "watch": "webpack --watch --env.dev",
    "prebuild": "npm run clean.dist & npm run build.dll",
    "build": "webpack --env.dev",
    "prebuild.prod": "npm run clean.dist & npm run build.dll.prod",
    "build.prod": "webpack --env.prod",
    "prebuild.dll": "rimraf dist/dll/*.*",
    "build.dll": "webpack --config config/webpack.dll.config.ts --env.dev",
    "prebuild.dll.prod": "rimraf dist/dll/*.*",
    "build.dll.prod": "webpack --config config/webpack.dll.config.ts --env.prod",
    "start": "npm run start.dev",
    "prestart.dev": "npm run build",
    "start.dev": "webpack-dev-server --inline --progress --port 8080 --env.dev",
    "start.hmr": "npm run start.dev -- --env.hot",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "symlink": "node config/symlink.js"
  },
  "private": true,
